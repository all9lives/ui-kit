{
  "name": "core_ui_kit",
  "version": "1.1.0",
  "main": "./lib/main.js",
  "private": true,
  "scripts": {
    "prepare": "npm run build",
    "start:native": "node node_modules/react-native/local-cli/cli.js start",
    "precommit": "lint-staged",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:js": "standard --verbose | snazzy",
    "lint:js-fix": "standard --fix --verbose | snazzy",
    "lint:css": "stylelint './src/**/*.js'",
    "typecheck": "flow",
    "generate:component": "babel-node helpers/scripts/run generator/component",
    "generate:story": "babel-node helpers/scripts/run generator/story",
    "storybook:web": "start-storybook -p 6006 -c storybook/web",
    "build-storybook:web": "build-storybook -c storybook/web",
    "storybook:native": "storybook start -p 7007 -c storybook/native",
    "ncu": "ncu",
    "ncu-force-update": "ncu -au"
  },
  "dependencies": {
    "flow-bin": "^0.54.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "16.0.0-alpha.12",
    "react-dom": "16.0.0-alpha.12",
    "react-native": "0.48.3",
    "react-primitives": "^0.4.3",
    "recompose": "^0.25.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "@storybook/react-native": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^21.0.2",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "husky": "^0.14.3",
    "jest": "21.1.0",
    "jest-styled-components": "^4.6.0",
    "lint-staged": "^4.2.1",
    "ncu": "^0.2.1",
    "npm-check-updates": "^2.12.1",
    "react-test-renderer": "16.0.0-beta.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^1.0.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "preset": "react-native",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./helpers/jest/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "helpers/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "helpers/"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "flow/"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": "stylelint-config-standard",
    "syntax": "scss",
    "rules": {
      "no-empty-source": null
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "stylelint",
      "git add"
    ]
  }
}
